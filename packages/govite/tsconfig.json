{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"declaration": true,
		"lib": ["DOM"],
		"noFallthroughCasesInSwitch": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "dist",
		"skipLibCheck": true,
		"strict": true
	},
	"extends": "@tsconfig/node20",
	"include": ["src"]
}
